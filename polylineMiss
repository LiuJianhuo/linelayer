  <div id="map_container"></div>
  <div style="position: fixed;top:20px;padding:20px;background:#fff;width:100%;z-index: 100;">
    <button onclick="addPolylines()">添加线路</button>
    <button onclick="window.location.reload()">reload</button>
  </div>



<script>
    var map = new BMapGL.Map('map_container', {

    });

    map.enableKeyboard();
    map.enableScrollWheelZoom();
    map.enableInertialDragging();
    map.enableContinuousZoom();
    map.setMapStyleV2({styleJson: theme});
    const center = new BMapGL.Point(121.30654632240122, 31.25744185633306)
    map.centerAndZoom(center,  12);



    function addPolylines() {

      let allPoints = [];
      lines.map((points) => {
        allPoints = allPoints.concat(points)
      })


      const polylines = lines.map((points, index) => {
        return new BMapGL.Polyline(points, {
            strokeWeight: 7,
            strokeColor: index === 0 ? 'blue' : 'red',
            // strokeTexture: {
            //   url:index === 0?'https://mapopen-pub-jsapigl.bj.bcebos.com/svgmodel/Icon_road_red_arrow.png':
            //               'https://mapopen-pub-jsapigl.bj.bcebos.com/svgmodel/Icon_road_blue_arrow.png',
            //   width: 64,
            //   height: 256,
            // }
        })
      })

      polylines.forEach((item) => {
          map.addOverlay(item)
      })

      // 设置最佳视野
      map.setViewport(allPoints)


      // 移除
      polylines.forEach((item, index) => {
          map.removeOverlay(item)
      })

      // 再添加操作
      polylines.forEach((item) => {
          map.addOverlay(item)
        //   item.show()
      })

    }


</script>

</body>
</html>




